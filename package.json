    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.23.0",
    "express": "^4.19.2",
    "jsdom": "^24.0.0",
    "msw": "^2.3.0",
    "npm-run-all2": "^6.1.2",
    "openapi-zod-client": "^1.18.1",
    "playwright": "^1.44.0",
    "playwright-msw": "^3.0.1",
    "swagger-ui-express": "^5.0.0",
    "typescript": "~5.3.3",
    "unocss": "^0.58.6",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-vue-components": "^0.26.0",
