    "axios": "^1.6.8",
    "oh-vue-icons": "1.0.0-rc3",
    "pinia": "^2.1.7",
    "vee-validate": "^4.12.6",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0",
    "vue-tel-input": "^9.1.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.8.3",
    "@asteasolutions/zod-to-openapi": "^7.0.0",
